{
  "name": "qqwry-ip-api-server",
  "version": "1.0.0",
  "description": "基于纯真IP库的在线IP查询API服务",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "update-db": "node scripts/update-db.js",
    "test": "node test/test.js",
    "test:unit": "node test/unit.test.js",
    "test:all": "npm run test:unit && npm run test",
    "install-deps": "npm install",
    "pm2-start": "pm2 start ecosystem.config.js",
    "pm2-stop": "pm2 stop qqwry-api",
    "pm2-restart": "pm2 restart qqwry-api",
    "docker:build": "docker build -t qqwry-api .",
    "docker:run": "docker run -p 3000:3000 -d --name qqwry-api qqwry-api",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "ip",
    "geolocation",
    "qqwry",
    "api",
    "nodejs",
    "纯真IP库"
  ],
  "author": "CodeBuddy",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "node-cache": "^5.1.2",
    "axios": "^1.6.0",
    "iconv-lite": "^0.6.3",
    "fs-extra": "^11.1.1",
    "cron": "^3.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/qqwry-ip-api-server.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/qqwry-ip-api-server/issues"
  },
  "homepage": "https://github.com/your-username/qqwry-ip-api-server#readme"
}